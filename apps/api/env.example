# Celery / Redis
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1
CELERY_DEFAULT_QUEUE=one_to_all

# Postgres (job history)
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/one_to_all

# Runtime directories (local dev only; data/ is temporary)
DATA_DIR=data
MODELS_DIR=models
ONE_TO_ALL_MODEL_DIR=One-to-All-14b-FP8
ONE_TO_ALL_MODEL_REPO_ID=MochunniaN1/One-to-All-14b

# One-to-All-Animation base model (Diffusers folder)
WAN_T2V_14B_DIFFUSERS_DIR=models/One-to-All-Animation/pretrained_models/Wan2.1-T2V-14B-Diffusers

# Optional S3/MinIO (recommended for persistent artifacts)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET_NAME=one-to-all
S3_REGION=us-east-1
S3_SECURE=false
S3_PREFIX=one-to-all

# third_party location (if worker runs One-to-All-Animation scripts)
ONE_TO_ALL_ANIMATION_DIR=third_party/One-to-All-Animation

# One-to-All-Animation runtime/assets directory (keep third_party read-only)
ONE_TO_ALL_ANIMATION_RUNTIME_DIR=models/One-to-All-Animation
ONE_TO_ALL_ANIMATION_PRETRAINED_DIR=models/One-to-All-Animation/pretrained_models
